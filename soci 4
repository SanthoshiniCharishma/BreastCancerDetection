orm(X_train, y_train)\n","      X_test_selected = selector.transform(X_test)\n","      classifier.fit(X_train_selected, y_train)\n","      predictions = classifier.predict(X_test_selected)\n","      accuracy = accuracy_score(y_test, predictions)\n","      print(f\"Accuracy of filter type {filter_name}: {accuracy:.3f}\")\n","      y_pred = classifier.predict(X_test_selected)\n","      report = classification_report(y_test, y_pred)\n","      print(\"Classification Report with selected features:\\n\", report)"]},{"cell_type":"code","source":["filter_methods = [\n","    ('Variance Threshold', VarianceThreshold(threshold=0.2))\n","]\n","\n","for filter_name, filter_method in filter_methods:\n","    selector = filter_method\n","    X_train_selected = selector.fit_transform(X_train, y_train)\n","    X_test_selected = selector.transform(X_test)"],"metadata":{"id":"ouWAjXVMZ0rB","executionInfo":{"status":"ok","timestamp":1711131871838,"user_tz":-330,"elapsed":383,"user":{"displayName":"Dudekula Reshma","userId":"13079973883530239740"}}},"execution_count":20,"outputs":[]},{"cell_type":"code","source":["from sklearn.metrics import roc_curve\n","base_classifiers = [\n","    ('gb', GradientBoostingClassifier(n_estimators=100, random_state=42)),\n","    ('knn', KNeighborsClassifier(n_neighbors=5))]\n","meta_classifier = LogisticRegression()\n","model = StackingClassifier(estimators=base_classifiers, final_estimator=meta_classifier)\n","model.fit(X_train_selected, y_train)\n","\n","# Predicting probabilities for the validation set\n","val_probabilities = model.predict_proba(X_test_selected)[:, 1]\n","\n","# Calculating TPR and FPR for various thresholds\n","fpr, tpr, thresholds = roc_curve(y_test, val_probabilities)\n","\n","from sklearn.metrics import auc\n","roc_auc = auc(fpr, tpr)\n","print(f\"AUC: {roc_auc}\")\n","\n","import matplotlib.pyplot as plt\n","# Plotting the ROC curve\n","plt.figure(figsize=(8, 6))\n","plt.plot(fpr, tpr, color='darkorange', lw=2, label=f'ROC curve (